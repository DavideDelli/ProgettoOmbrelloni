CREATE PROCEDURE PopolaDatabase()
BEGIN
    -- Variabili per i cicli e i dati da generare
    DECLARE i INT DEFAULT 1;
    DECLARE j INT DEFAULT 1;
    DECLARE v_id_ombrellone INT;
    DECLARE v_data_corrente DATE;
    DECLARE v_cod_tipologia VARCHAR(10);

    -- 1. POPOLAMENTO CLIENTI
    -- Inseriamo 50 clienti fittizi
    SET i = 1;
    WHILE i &lt;= 50 DO
        INSERT INTO Cliente (codice, nome, cognome, dataNascita, indirizzo)
        VALUES (
            CONCAT('CLIENTE', LPAD(i, 4, '0')), -- Crea un codice univoco tipo CLIENTE0001
            CONCAT('Nome', i),
            CONCAT('Cognome', i),
            DATE_SUB('2000-01-01', INTERVAL FLOOR(RAND() * 5000) DAY), -- Data di nascita casuale
            CONCAT('Via Inventata ', i)
        );
        SET i = i + 1;
    END WHILE;

    -- 2. POPOLAMENTO OMBRELLONI
    -- Inseriamo 100 ombrelloni, partendo dall'ID 200 per non sovrapporci a quelli esistenti
    SET i = 200;
    WHILE i &lt;= 299 DO
        -- Assegna una tipologia: 80% Standard, 20% VIP
        IF RAND() &lt; 0.8 THEN
            SET v_cod_tipologia = 'STD';
        ELSE
            SET v_cod_tipologia = 'VIP';
        END IF;

        INSERT INTO Ombrellone (id, settore, numFila, numPostoFila, codTipologia)
        VALUES (
            i,
            CHAR(65 + FLOOR(RAND() * 5)), -- Settore casuale da 'A' a 'E'
            FLOOR(1 + RAND() * 10),      -- Fila casuale da 1 a 10
            FLOOR(1 + RAND() * 15),      -- Posto casuale da 1 a 15
            v_cod_tipologia
        );
        SET i = i + 1;
    END WHILE;

    -- 3. POPOLAMENTO GIORNIDISPONIBILITA
    -- Per ogni nuovo ombrellone (ID da 200 a 299), inseriamo la disponibilit√†
    -- per tutto Luglio e Agosto 2025.
    SET v_id_ombrellone = 200;
    WHILE v_id_ombrellone &lt;= 299 DO
        SET v_data_corrente = '2025-07-01';
        WHILE v_data_corrente &lt;= '2025-08-31' DO
            -- Inseriamo il giorno come disponibile (nessun contratto associato)
            INSERT INTO GiornoDisponibilita(idOmbrellone, data, numProgrContratto)
            VALUES (v_id_ombrellone, v_data_corrente, NULL);

            -- Passa al giorno successivo
            SET v_data_corrente = DATE_ADD(v_data_corrente, INTERVAL 1 DAY);
        END WHILE;
        SET v_id_ombrellone = v_id_ombrellone + 1;
    END WHILE;

END$$

